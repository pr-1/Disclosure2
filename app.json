{
  "expo": {
    "name": "Disclosure Hub",
    "slug": "disclosure-hub",
    "version": "2.0.5",
    "orientation": "portrait",
    "icon": "./assets/disclosurelogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/white.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.foxwylie.disclosure-hub",
      "buildNumber": "2.0.5",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show you the offers and participating businesses closest to you."
      }
    },
    "android": {
      "package": "com.foxwylie.disclosure_hub",
      "versionCode": 25,
      "adaptiveIcon": {
        "foregroundImage": "./assets/white.png",
        "backgroundColor": "#000"
      },
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
