{
  "expo": {
    "name": "Disclosure Hub",
    "slug": "disclosure-hub",
    "version": "2.0.9",
    "orientation": "portrait",
    "icon": "./assets/disclosurelogo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/white.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.foxwylie.disclosure-hub",
      "buildNumber": "8",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to show you the offers and participating businesses closest to you."
      }
    },
    "android": {
      "package": "com.foxwylie.disclosure_hub",
      "credentialsSource": "local",
      "versionCode": 31,
      "adaptiveIcon": {
        "foregroundImage": "./assets/white.png",
        "backgroundColor": "#000"
      },
      "keystore": {
        "keystorePath": "keystore/release.keystore",
        "keystorePassword": "secret@123",
        "keyAlias": "key",
        "keyPassword": "secret@123"
      },  
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6b7c50c4-1e20-4205-a23a-a876c46b5665"
      }
    }
  }
}
